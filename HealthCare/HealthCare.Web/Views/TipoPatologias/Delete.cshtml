@model HealthCare.Web.Models.TipoPatologia

@{
    ViewData["Title"] = "Tipo de Patologías";
    ViewData["TituloOperacion"] = "¿Desea eliminar Tipo de Patología?";    
}

<div>    
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>

        
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-primary" />
        <a class="btn btn-primary " asp-action="Index">Volver a Lista</a>
    </form>
</div>
